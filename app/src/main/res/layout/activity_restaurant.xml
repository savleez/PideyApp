<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".RestaurantActivity">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="106dp"
        app:layout_constraintBottom_toTopOf="@+id/editTextNomRestaurant"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logopideyapp" />

    <TextView
        android:id="@+id/TxtRestDisponibles"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="33dp"
        android:text="Restaurantes"
        android:textColor="#F60000"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/imageView6"
        app:layout_constraintEnd_toStartOf="@+id/imageView5"
        app:layout_constraintStart_toStartOf="@+id/imageView5"
        app:layout_constraintTop_toBottomOf="@+id/imageView5" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="31dp"
        android:layout_marginBottom="44dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/BotonVerMenu"
        app:layout_constraintTop_toBottomOf="@+id/TxtRestDisponibles"
        tools:srcCompat="@tools:sample/avatars" />

    <EditText
        android:id="@+id/editTextNomRestaurant"
        android:layout_width="225dp"
        android:layout_height="0dp"
        android:layout_marginTop="197dp"
        android:layout_marginBottom="1dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Nombre Restaurante"
        app:layout_constraintBottom_toTopOf="@+id/BotonVerMenu"
        app:layout_constraintEnd_toEndOf="@+id/BotonVerMenu"
        app:layout_constraintStart_toEndOf="@+id/BotonVerMenu"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/BotonVerMenu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="43dp"
        android:layout_marginEnd="136dp"
        android:layout_marginBottom="83dp"
        android:backgroundTint="#E30613"
        android:onClick="vermenurest"
        android:text="Ver Menu"
        app:layout_constraintBottom_toTopOf="@+id/botonVerMenu"
        app:layout_constraintEnd_toStartOf="@+id/imageView6"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextNomRestaurant" />

    <EditText
        android:id="@+id/editTextNomRestaurant3"
        android:layout_width="231dp"
        android:layout_height="63dp"
        android:layout_marginBottom="95dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Nombre Restaurante"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/botonVerMenu"
        app:layout_constraintStart_toEndOf="@+id/botonVerMenu"
        app:layout_constraintTop_toBottomOf="@+id/editTextNomRestaurant2" />

    <Button
        android:id="@+id/botonVerMenu2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="43dp"
        android:layout_marginEnd="144dp"
        android:layout_marginBottom="47dp"
        android:backgroundTint="#E30613"
        android:text="Ver Menu"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/imageView8"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/botonVerMenu" />

    <Button
        android:id="@+id/botonVerMenu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="43dp"
        android:layout_marginEnd="144dp"
        android:layout_marginBottom="76dp"
        android:backgroundTint="#E30613"
        android:onClick="vermenurest"
        android:text="Ver Menu"
        app:layout_constraintBottom_toTopOf="@+id/botonVerMenu2"
        app:layout_constraintEnd_toStartOf="@+id/imageView7"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/BotonVerMenu" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="23dp"
        android:layout_marginBottom="63dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/botonVerMenu2"
        app:layout_constraintTop_toBottomOf="@+id/imageView7"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="23dp"
        android:layout_marginBottom="38dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/botonVerMenu"
        app:layout_constraintTop_toBottomOf="@+id/imageView6"
        tools:srcCompat="@tools:sample/avatars" />

    <EditText
        android:id="@+id/editTextNomRestaurant2"
        android:layout_width="235dp"
        android:layout_height="81dp"
        android:layout_marginBottom="70dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Nombre Restaurante"
        app:layout_constraintBottom_toTopOf="@+id/editTextNomRestaurant3"
        app:layout_constraintEnd_toEndOf="@+id/BotonVerMenu"
        app:layout_constraintHorizontal_bias="0.472"
        app:layout_constraintStart_toEndOf="@+id/BotonVerMenu"
        app:layout_constraintTop_toBottomOf="@+id/editTextNomRestaurant" />

</androidx.constraintlayout.widget.ConstraintLayout>